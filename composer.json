{
    "name": "kernery/core",
    "description": "Open source framework built on laravel with extensible battery included.",
    "keywords": [
        "laravel",
        "kernery",
        "php",
        "laravel kernery"
    ],
    "homepage": "https://kernery.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kernery",
            "email": "hello@kernery.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "kernery/asset": "*@dev",
        "wikimedia/composer-merge-plugin": "^2.1.0",
        "tightenco/ziggy": "2.5.3",
        "kris/laravel-form-builder": "^1.54.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.16.0"
    },
    "autoload": {
        "psr-4": {
            "Kernery\\Main\\": "main/src",
            "Tightenco\\Ziggy\\": "main/src/Ziggy",
            "Kernery\\Main\\Tests\\": "main/tests",
            "Kernery\\Icon\\": "icon/src",
            "Kernery\\Form\\": "form/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kernery\\Main\\Supports\\ServiceProvider",
                "Kernery\\Main\\Providers\\MainServiceProvider",
                "Kernery\\Main\\Providers\\CommandServiceProvider",
                "Kernery\\Icon\\Providers\\IconServiceProvider",
                "Kernery\\Form\\Providers\\FormServiceProvider"
            ]
        },
        "aliases": {
            "AppHelper": "Kernery\\Main\\Facades\\AppHelper",
            "AppIcon": "Kernery\\Icon\\Facades\\Icon"
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}